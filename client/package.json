{
  "name": "spark-starter-kit",
  "version": "0.9.0",
  "description": "Spark Solutions starter kit",
  "main": "server-express.js",
  "engines": {
    "node": "5.1.0",
    "npm": "3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparks-solutions/spark-starter-kit.git"
  },
  "keywords": [
    "react",
    "tutorial",
    "comment",
    "example"
  ],
  "author": "killy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparks-solutions/spark-starter-kit/issues"
  },
  "homepage": "https://github.com/sparks-solutions/spark-starter-kit",
  "scripts": {
    "test": "NODE_PATH=./js mocha --compilers js:babel-core/register --require ./js/libs/testHelper.js --require ./js/libs/testNullCompiler.js './js/**/*.spec.@(js|jsx)'",
    "test:debug": "npm run test -- --debug-brk",
    "start": "node server-express.js",
    "build:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:dev:client": "babel-node server-rails-hot.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "build:test:client": "webpack -w --config webpack.client.rails.build.config.js",
    "lint": "npm run eslint && npm run jscs",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose ."
  },
  "dependencies": {
    "autoprefixer": "^6.1.2",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap-loader": "^1.0.0-rc",
    "bootstrap-sass": "^3.3.5",
    "css-loader": "^0.23.0",
    "es5-shim": "^4.3.1",
    "es6-promise": "^3.0.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "history": "^1.13.1",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.0",
    "jquery": "^2.1.4",
    "jquery-ujs": "^1.1.0-1",
    "loader-utils": "^0.2.11",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.7",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redux": "^3.0.4",
    "redux-form": "^4.0.5",
    "redux-thunk": "^1.0.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.1",
    "sass-resources-loader": "1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "body-parser": "^1.14.1",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.4",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jscs": "^2.7.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.3.1",
    "should": "^8.0.2",
    "sleep": "^3.0.0",
    "webpack-dev-server": "^1.12.1"
  }
}
